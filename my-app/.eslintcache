umbs>\n          {imgs.map((img, index)=>{\n        return (<img className=\"images\" src={img} key={index} alt={img}/>)})}\n          </Carousel>\n          <Card.Text>Asking Price:<span className='green'> $ </span>{itemPrice}</Card.Text>\n          <Card.Text><span className='scriptHeader'>Owner Comments: </span><span className='commentBox'>{itemDescription}</span></Card.Text>\n          {renderContact()}    \n        </div>\n        ) : null }\n      </div>\n      \n  )\n}\n\nexport default PostDetails","/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/components/pages/Posts/EditPost.jsx",[],"/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/firebase.jsx",[],"/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/components/static/Contact.jsx",[],"/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/components/static/About.jsx",[],"/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/DataContext.js",[],"/Users/MarlenaTsang/Documents/Development/Code/Mod5/e-mart/my-app/src/components/pages/Posts/PostsContainer.jsx",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":11,"column":8,"nodeType":"120","messageId":"121","endLine":11,"endColumn":19},{"ruleId":"118","severity":1,"message":"122","line":12,"column":8,"nodeType":"120","messageId":"121","endLine":12,"endColumn":16},{"ruleId":"114","replacedBy":"123"},{"ruleId":"116","replacedBy":"124"},{"ruleId":"125","severity":1,"message":"126","line":40,"column":7,"nodeType":"127","endLine":40,"endColumn":9,"suggestions":"128"},{"ruleId":"125","severity":1,"message":"126","line":45,"column":5,"nodeType":"127","endLine":45,"endColumn":7,"suggestions":"129"},{"ruleId":"114","replacedBy":"130"},{"ruleId":"116","replacedBy":"131"},{"ruleId":"118","severity":1,"message":"132","line":8,"column":12,"nodeType":"120","messageId":"121","endLine":8,"endColumn":20},{"ruleI